##create database celulares;

##use celulares;

create table xiaomi(
    idNum int primary key auto_increment,
	Model varchar(100),
    valor float
);

create table apple(
    idNum int primary key auto_increment,
	Model varchar(100),
    valor float
);

create table motorola(
    idNum int primary key auto_increment,
	Model varchar(100),
    valor float
);

create table lg(
    idNum int primary key auto_increment,
	Model varchar(100),
    valor float
);

create table samsung(
    idNum int primary key auto_increment,
	Model varchar(100),
    valor float
);

